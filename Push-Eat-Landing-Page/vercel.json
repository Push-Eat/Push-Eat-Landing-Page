{
  "functions": {
    "api/deal-meta.js": {
      "runtime": "@vercel/edge"
    }
  },
  "routes": [
    {
      "src": "/deal/([^/]+)",
      "dest": "/api/deal-meta?dealId=$1",
      "headers": {
        "cache-control": "public, s-maxage=300, stale-while-revalidate=86400"
      }
    },
    {
      "src": "/.well-known/(.*)",
      "dest": "/.well-known/$1",
      "headers": {
        "cache-control": "public, max-age=86400"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/.well-known/assetlinks.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        },
        {
          "key": "Cache-Control", 
          "value": "public, max-age=86400"
        }
      ]
    },
    {
      "source": "/deal/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/deal/:dealId",
      "destination": "/api/deal-meta"
    }
  ]
}